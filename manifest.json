{
    "manifest_version": 2,
  
    "name": "Blokka íslenskar rusl síður",
    "description": "Losaðu þig við rusl síður",
    "version": "0.2",
  
    "browser_action": {
      "default_icon": "img/icon.png",
      "default_popup": "popup.html",
      "default_title": "Haltu sjálfsvirðingu"
    },

    "background": { 
        "scripts": ["js/main.js"]
    },

    "permissions": [
      "activeTab",
      "storage",
      "webRequest",
      "webNavigation",
      "webRequestBlocking",
      "*://*/*"
    ]
  }
